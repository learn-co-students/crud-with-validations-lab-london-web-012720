<p>Form</p>

<%= form_for @song do |f|%>
<%= f.label :title %>
<%= f.text_field :title %><div>
<%= f.label :release_year %>
<%= f.number_field :release_year %><div>
<%= f.label :released %>
<%= f.check_box :released %><div>
<%= f.label :genre %>
<%= f.text_field :genre %><div>
<%= f.label :artist_name %>
<%= f.text_field :artist_name %><div>
<%= f.submit%>
<%end%>